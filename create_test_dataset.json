{
    "num_workers": 4,
    "preprocessing": {
        "sr": 16000,
        "spectrogram": {
            "type": "MelSpectrogram",
            "args": {
                "n_mels": 256
            }
        },
        "log_spec": true
    },
    "data": {
        "test": {
            "datasets": [
                {
                    "type": "LibrispeechDataset",
                    "args": {
                        "part": "test-clean"
                    }
                }
            ],
            "mixture_generator_init": {
                "test": true,
                "out_folder": "data/mixture/test",
                "nfiles": 100
            },
            "mixture_generator_generate_mixes": {
                "num_workers": 4,
                "update_steps": 100,
                "trim_db": null,
                "vad_db": 20,
                "audioLen": 3
            }
        }
    }
}