{
    "name": "one_batch_test",
    "n_gpu": 1,
    "text_encoder": {
        "type": "CTCCharTextEncoder"
    },
    "preprocessing": {
        "sr": 16000,
        "spectrogram": {
        "type": "MelSpectrogram",
        "args": {
            "n_mels": 512
        }
        },
        "log_spec": true
    },
    "augmentations": {
        "random_apply_p": 0.5,
        "wave": [],
        "spectrogram": []
    },
    "arch": {
        "type": "SpExPlusModel",
        "args": {
        "L1": 20, 
        "L2": 40, 
        "L3": 80, 
        "N": 256, 
        "ResNetBlock_cnt": 3,
        "TCN_cnt": 8
        }
    },
    "data": {
        "train": {
        "batch_size": 2,
        "num_workers": 0,
        "datasets": [
            {
            "type": "MixtureDataset",
            "args": {
                "part": "train-clean-100-mixed",
                "max_audio_length": 20.0,
                "limit": 10
            }
            }
        ]
        }
    },
    "optimizer": {
        "type": "AdamW",
        "args": {
        "lr": 3e-4,
        "weight_decay": 0
        }
    },
    "loss": {
        "type": "SpExPlusLoss",
        "args": {
            "alpha": 0.1,
            "beta": 0.1,
            "gamma": 0.5
        }
    },
    "metrics": [
        {
            "type": "PESQMetric",
            "args": {
                "name": "PESQ"
            }
        },
        {
            "type": "SISDRMetric",
            "args": {
                "name": "SI-SDR"
            }
        }
    ],
    "lr_scheduler": {
        "type": "OneCycleLR",
        "args": {
        "steps_per_epoch": 100,
        "epochs": 10,
        "anneal_strategy": "cos",
        "max_lr": 1e-3,
        "pct_start": 0.2
        }
    },
    "trainer": {
        "epochs": 10,
        "save_dir": "saved/",
        "save_period": 5,
        "verbosity": 2,
        "monitor": "min val_loss",
        "early_stop": 100,
        "visualize": "wandb",
        "wandb_project": "dla2_ss_project_check",
        "len_epoch": 100,
        "grad_norm_clip": 100
    }
}
